{
    "[typescript]": {
      "editor.formatOnSave": true
    },
    "[javascript]": {
      "editor.formatOnSave": true
    },
    "[json]": {
      "editor.formatOnSave": false
    },
    "[jsonc]": {
      "editor.formatOnSave": false
    },
    "files.insertFinalNewline": true,
    "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "node_modules": true,
      "**/node_modules": true,
      "dist": true,
      "**/dist": true
    }
  
  }
  